        <div class="col-sm-3">
            <div class="row">
                <div class="col-sm-12">
                    <div class="portlet light">
                        <div class="portlet-body">
                            <input type="text" class="form-control" id="searchItemQuery" placeholder="Scan Box" />
                        </div>
                    </div>
                    <div class="portlet light">
                        <div class="portlet-body">
                            <input id="code" class="codeBox form-control" type="text" onkeyup="getCode(event)" placeholder="Code Box" autocomplete="off" />
                        </div>
                    </div>


                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="portlet light">
                        <div class="portlet-body">
                            <div class="row">
                                <div style="float:left;width:50%;font-size:20px;font-weight:bolder;">
                                    Bill: <span id="order_no" class="order-number">@Model.OrderNo</span>
                                </div>
                                <div style="float:right;width:50%;text-align:right;font-size:20px;font-weight:bolder;">
                                    <span class="order-number">@DateTime.Today.ToShortDateString()</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="portlet light summaryDiv">
                        <div class="portlet-body">
                            <div class="row subtotal-row">
                                <div style="float:left">
                                    <h3 class="subtotal">@POS.Resources.Resource.SubTotal</h3>
                                </div>
                                <div style="float:right">
                                    <h3 class="subtotal" id="saleModel_SubTotal">$0</h3>
                                </div>
                            </div>
                            <div class="row subtotal-row">
                                <div style="float:left">
                                    <h3 class="subtotal">Sales Tax</h3>
                                </div>
                                <div style="float:right">
                                    <h3 class="subtotal" id="saleModel_GST">$0</h3>
                                </div>
                            </div>
                            <div class="row subtotal-row">
                                <div style="float:left">
                                    <h3 class="subtotal">Discount</h3>
                                </div>
                                <div style="float:right;cursor:pointer;text-align:center;" onclick="addDiscount()">
                                    <h3 class="saleModel_Discount subtotal">0%</h3>
                                </div>
                                <div id="discountWindow" style="overflow:hidden;display:none;">
                                    <div class="row">
                                        <div class="col-sm-11">
                                            <div class="portlet light">
                                                <div class="portlet-body">
                                                    <h2 class="saleModel_Discount">0%</h2>
                                                </div>
                                            </div>
                                            <div class="portlet light">
                                                <div class="portlet-body">
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(7)">7</button>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(8)">8</button>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(9)">9</button>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(4)">4</button>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(5)">5</button>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(6)">6</button>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(1)">1</button>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(2)">2</button>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(3)">3</button>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount('.')">.</button>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount(0)">0</button>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <button type="button" class="btn numeric-btn btn-default" onclick="setDiscount('00')">00</button>
                                                        </div>
                                                    </div>
