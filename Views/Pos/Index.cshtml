@using DataAccessLayer.Helpers

@model POS.Models.Pos.PosVM
@{
    ViewBag.Title = "POS Terminal";
    Layout = "~/Views/Shared/_LayoutPos.cshtml";
}

<div class="page-header navbar navbar-fixed-top">
    <div class="page-header-inner ">
        <div class="page-logo" style=" font-size: 25px; font-weight: bold; padding-top: 25px;">
            TechCity
        </div>
        <div class="location-name">
            <b>@Model.LocationName</b>
        </div>

        <div class="page-actions" style="float:right;">
            <div style="float:left;margin:5px;">
                <a href="#" class="btn btn-circle red btn-menu-lg" onclick="printLastInvoice()"><i class="fa fa-usd"></i> Print Last Invoice </a>
            </div>
            <div style="float:left;margin:5px;">
                <div class="btn-group btn-menu-lg">
                    <a class="btn btn-circle purple" href="javascript:;" data-toggle="dropdown" onclick="keyPressBeep()">
                        <i class="fa fa-gear"></i> Settings
                        <i class="fa fa-angle-down"></i>
                    </a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="javascript:;" onclick="OpeniningClosingScreen()">
                                <i class="fa fa-folder-open"></i> Opening/Closing
                            </a>
                        </li>
                        <li>
                            <a href="javascript:;" onclick="SalesScreen()">
                                <i class="fa fa-list"></i> @POS.Resources.Resource.Sale
                            </a>
                        </li>
                        <li>
                            <a href="javascript:;" onclick="RepairOrdersScreen()">
                                <i class="fa fa-list"></i> Repair Orders
                            </a>
                        </li>
                        @*<li class="divider"> </li>*@
                        @*<li>
                        <a href="@Url.Action("ChangeLanguage", "Language", new { langabb = "en-AU", redirectTo = "Pos" })">
                            <span class="lang-sm lang-lbl" lang="en" onclick="changeCulture('en-AU', 'd/MM/yyy')"></span>
                        </a>
                    </li>*@
                        @*<li>
                        <a href="@Url.Action("ChangeLanguage", "Language", new { langabb = "ar-SA", redirectTo = "Pos" })">
                            <span class="lang-sm lang-lbl" lang="ar" onclick="changeCulture('ar-AE', 'd/MM/yyy')"></span>
                        </a>
                    </li>*@
                    </ul>
                </div>
            </div>
            <div style="float:left;margin:5px;">
                <a href="@Url.Action("LogOff", "Login")" class="btn btn-circle red btn-menu-lg" onclick="keyPressBeep()"><i class="fa fa-sign-out"></i> @POS.Resources.Resource.LogOut </a>
            </div>
        </div>

    </div>
</div>
<div id="opening_closing_screen" class="not_main_screen content-screen">
</div>
<div id="sales_screen" class="not_main_screen content-screen">
</div>
<div id="sales_refund_screen" class="not_main_screen content-screen">
</div>
<div id="repair_orders_screen" class="not_main_screen content-screen">
</div>
<div id="print_screen" class="not_main_screen content-screen">
</div>
<div class="content-screen" id="main_screen">
    <div class="row">
        <div class="col-sm-9">
            <div class="row" style="display:none" id="search-grid-content">
                <div class="col-sm-12">
                    <div class="portlet light">
                        <div class="portlet-body">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="portlet light no-padding">
                                        <div class="portlet-body no-padding">
                                            <div class="row no-margin">
                                                <div style="float:left;width:100%;font-size:20px;font-weight:bolder;">
                                                    Searched Items
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div id="searchGrid"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-12">
                    <div class="portlet light">
                        <div class="portlet-body">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="portlet light no-padding">
                                        <div class="portlet-body no-padding">
                                            <div style="display:flex;justify-content:space-between;" class="no-margin">
                                                <div style="float:left;width:100%;font-size:20px;font-weight:bolder;">
                                                    Items
                                                </div>
                                                @*<button type="button" class="btn green" onclick="openNewCartItemWindow()" title="alt+n">Add New</button>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div id="cartGridWrapper">
                                        <div id="cartGrid"></div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="cartItemActionWindow" style="display:none;">
                    <div style="display:flex;justify-content:space-between;padding:20px">
                        <button type="button"
                                class="btn"
                                style="background-color:darkred;color:white"
                                onclick="removeCartItem()" title="alt+d">
                            Remove Item
                        </button>
                        <button type="button" class="btn" onclick="closeCartItemActionWindow()">X</button>
                    </div>
                    <div class="row">
                        @*Quantity*@
                        <div class="col-sm-4">
                            <div class="portlet light">
                                <div class="portlet-body">
                                    @*Qty: <h2 class="currentItem_Qty">0</h2>*@
                                    <input name="currentItem_Qty" class="currentItem_Qty form-control" tabindex="1"
                                           style="width: -webkit-fill-available;" id="currentItem_Qty" type="number" placeholder="Quantity" onkeyup="currentItem_QtyOnChange(this.value)" />
                                </div>
                            </div>
                            <div class="portlet light">
                                <div style="display:flex;justify-content:space-between;padding:20px">
                                    <button type="button" class="btn cart-item-btn green" onclick="increaseQty()">+</button>
                                    <button type="button" class="btn cart-item-btn blue" onclick="decreaseQty()">-</button>
                                </div>
                            </div>
                            <div class="portlet light">
                                <div class="portlet-body">
                                    <div class="row">
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(7)" tabindex="-1">7</button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(8)" tabindex="-1">8</button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(9)" tabindex="-1">9</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(4)" tabindex="-1">4</button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(5)" tabindex="-1">5</button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(6)" tabindex="-1">6</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(1)" tabindex="-1">1</button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(2)" tabindex="-1">2</button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(3)" tabindex="-1">3</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-offset-4 col-sm-4">
                                            <button type="button" class="btn numeric-btn btn-qty btn-default" onclick="setItemQuantity(0)" tabindex="-1">0</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <button type="button" class="btn clear-btn btn-qty btn-default" style="width:95% !important" onclick="setItemQuantity('clear')" tabindex="-1">Clear</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        @*Price*@
                        <div class="col-sm-5">
                            <div class="portlet light">
                                <div class="portlet-body">
                                    Price: <input type="number" tabindex="2"
                                                  class="form-control"
                                                  id="cartItemPriceInput"
                                                  onkeyup="setCartItemPrice(this.value)" />
                                </div>
                            </div>
                            <div class="portlet light">
                                <div class="portlet-body">
                                    <div class="row">
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(7)">
                                                7
                                            </button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(8)">
                                                8
                                            </button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(9)">
                                                9
                                            </button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(4)">
                                                4
                                            </button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(5)">
                                                5
                                            </button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(6)">
                                                6
                                            </button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(1)">
                                                1
                                            </button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(2)">
                                                2
                                            </button>
                                        </div>
                                        <div class="col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(3)">
                                                3
                                            </button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-offset-4 col-sm-4">
                                            <button type="button" tabindex="-1"
                                                    class="btn numeric-btn btn-qty btn-default"
                                                    onclick="setItemPrice(0)">
                                                0
                                            </button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <button type="button" tabindex="-1"
                                                    class="btn clear-btn btn-qty btn-default"
                                                    style="width:95% !important" onclick="setItemPrice('clear')">
                                                Clear
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        @*Item Tax*@
                        <div class="col-sm-2">
                            <div class="portlet light">
                                <div class="portlet-body">
                                    Tax: <input type="number" tabindex="3"
                                                class="form-control"
                                                id="cartItemTaxInput"
                                                onkeyup="setCartItemTax(this.value)" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="newCartItemWindow" style="display:none;">
                    <div style="display:flex;justify-content:space-between;padding:20px">
                        <h2>New Item</h2>
                        <button type="button" class="btn" onclick="closeNewCartItemWindow()">X</button>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            <div style="display:flex;flex-direction:column">
                                <label>Name</label>
                                <input type="text" class="form-control" id="itemName" />
                            </div>
                            <div style="display:flex;flex-direction:column">
                                <label>Description</label>
                                <input type="text" class="form-control" id="itemDescription" />
                            </div>
                            <div style="display:none;flex-direction:column">
                                <label>IMEI/SN</label>
                                <input type="text" class="form-control" id="itemImei" />
                            </div>
                            <div style="display:flex;flex-direction:column">
                                <label>Sale Price</label>
                                <input type="number" class="form-control" id="itemSalePrice" />
                            </div>
                            <div style="display:flex;flex-direction:column">
                                <label>Purchase Cost</label>
                                <input type="number" class="form-control" id="itemPurchaseCost" />
                            </div>
                            <div style="display: none!important; flex-direction: column">
                                <label>Tax</label>
                                <input type="number" class="form-control" id="itemTax" value="13" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            <button type="button" class="btn green" onclick="addAnonymousCartItem()">Add Item</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div id="saleCreditAmountWindow" style="display:none;">
                    <div style="display:flex;justify-content:space-between;padding:20px">
                        <h2>Credit Sale</h2>
                        <button type="button" class="btn" onclick="closeSaleCrediAmountWindow()">X</button>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            <div style="display:flex;flex-direction:column">
                                <label>Credit Sale</label>
                                <input type="number" max="-1" class="form-control" id="saleCreditAmount" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            <button type="button" class="btn green" onclick="addSaleCreditAmount()">Add Credit Sale</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div id="serviceChargesWindow" style="display:none;">
                    <div style="display:flex;justify-content:space-between;padding:20px">
                        <h2>Service Charges</h2>
                        <button type="button" class="btn" onclick="closeServiceChargesWindow()">X</button>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            <div style="display:flex;flex-direction:column">
                                <label>Service Charges</label>
                                <input type="number" max="-1" class="form-control" id="serviceCharges" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            <button type="button" class="btn green" onclick="addServiceCharges()">Add Service Charges</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div id="IMEISNWindow" style="display:none;">
                    <div style="display:flex;justify-content:space-between;padding:20px">
                        <h2>IMEI/SN</h2>
                        <button type="button" class="btn" onclick="closeIMEISNWindow()">X</button>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            <div style="display:flex;flex-direction:column">
                                <label>IMEI/SN</label>
                                <input type="text" class="form-control" id="IMEISN" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            <button type="button" class="btn green" onclick="addIMEISN()">Add IMEI/SN</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="portlet light">
                    <div class="portlet-body InstructionList">
                        <h4>Instructions</h4>
                        <ul class="list-unstyled">
                            <li><strong>001</strong> - Add new Product</li>
                            <li><strong>002</strong> - Chart TOPUP</li>
                            <li><strong>003</strong> - Credit sale</li>
                            <li><strong>004</strong> - No Warranty On Physical Items Or Liquid Damage</li>
                            <li><strong>005</strong> - 30 Days Warranty</li>
                            <li><strong>006</strong> - 90 Days Warranty On TV</li>
                            <li><strong>007</strong> - Android Box</li>
                            <li><strong>009</strong> - Service Charges</li>
                            <li><strong>111</strong> - IMEI SN</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
